END CIMS_METADATA_MAINTAIN;

CREATE OR REPLACE PACKAGE BODY CIMS_METADATA_MAINTAIN AS


   PROCEDURE delimstring_to_table
    ( p_delimstring IN  VARCHAR2
    , p_table       OUT varchar2_table
    , p_nfields     OUT INTEGER
    , p_delim       IN  VARCHAR2 DEFAULT ','
    )
  IS
    v_string   VARCHAR2(32767) := p_delimstring;
    v_nfields  PLS_INTEGER := 1;
    v_table    varchar2_table;
    v_delimpos PLS_INTEGER := INSTR(p_delimstring, p_delim);
    v_delimlen PLS_INTEGER := LENGTH(p_delim);
  BEGIN
    WHILE v_delimpos > 0
    LOOP
      v_table(v_nfields) := SUBSTR(v_string,1,v_delimpos-1);
      v_string := SUBSTR(v_string,v_delimpos+v_delimlen);
      v_nfields := v_nfields+1;
      v_delimpos := INSTR(v_string, p_delim);
    END LOOP;
    v_table(v_nfields) := v_string;
    p_table := v_table;
    p_nfields := v_nfields;
  END delimstring_to_table;



    /**************************************************************************************************************************************
    * NAME:          createUserRoleAccess
    * DESCRIPTION:   insert rows into table USER_ROLE_ACCESS
    * Parameters :   assignTypeCodes is ',' delimited, like 'ASSIGNEE, OWNER_ASSIGNEE'
    *                languageCodes is ',' delimited, like 'ENG,FRA,ALL'
    *                resourceCodes is ',' delimited , like 'SECTION_REFERENCES,SECTION_Q_FOR_R_COMMENT'
    **************************************************************************************************************************************/
  PROCEDURE createUserRoleAccess(userRoleCode VARCHAR2, requestStatusId number,accessCode VARCHAR2,assignTypeCodes VARCHAR2,languageCodes VARCHAR2, resourceCodes VARCHAR2) AS
      assignTypeCode_array  varchar2_table;
      assignTypeCodes_len  BINARY_INTEGER;
      languageCode_array  varchar2_table;
      languageCodes_len  BINARY_INTEGER;
      resourceCode_array varchar2_table;
      resourceCodes_len  BINARY_INTEGER;
  BEGIN
    -- TODO: Implementation required for PROCEDURE CIMS_METADATA_MAINTAIN.createUserRoleAccess
     delimstring_to_table( assignTypeCodes ,assignTypeCode_array, assignTypeCodes_len,',');
     delimstring_to_table( languageCodes,languageCode_array,languageCodes_len,',');
     delimstring_to_table( resourceCodes,resourceCode_array, resourceCodes_len,',');


      FOR i IN 1 .. assignTypeCodes_len LOOP
         FOR j IN 1 .. languageCodes_len LOOP
           FOR k IN 1 .. resourceCodes_len LOOP
                -- dbms_output.put_line('element ' || i || ' is ' || assignTypeCode_array (i));
                 insert into USER_ROLE_ACCESS (USER_ROLE_CODE,CHANGE_REQUEST_STATUS_ID,ACCESS_CODE,ASSIGNMENT_TYPE_CODE,CHANGE_REQUEST_LANGUAGE_CODE,RESOURCE_CODE)
                 values (userRoleCode,requestStatusId,accessCode,assignTypeCode_array(i),languageCode_array(j),resourceCode_array(k));
           END LOOP;
         END LOOP;
      END LOOP;
  END createUserRoleAccess;


