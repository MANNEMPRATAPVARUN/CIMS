create table CIMS_REQUEST
(
  REQUEST_ID          NUMBER not null,
  REQUEST_NAME        VARCHAR2(200),
  CLASSIFICATION_CODE VARCHAR2(20),
  VERSION_CODE        VARCHAR2(4),
  LANGUAGE_CODE       VARCHAR2(3),
  REQUEST_STATUS_CODE VARCHAR2(2)
)
;
alter table CIMS_REQUEST
  add constraint PK_REQUEST primary key (REQUEST_ID);
alter table CIMS_REQUEST
  add constraint FK_REQUEST_CLASSIFICATION foreign key (CLASSIFICATION_CODE)
  references CIMS_CLASSIFICATION (CLASSIFICATION_CODE);
alter table CIMS_REQUEST
  add constraint FK_REQUEST_LANGUAGE foreign key (LANGUAGE_CODE)
  references CIMS_LANGUAGE (LANGUAGE_CODE);
alter table CIMS_REQUEST
  add constraint FK_REQUEST_STATUS foreign key (REQUEST_STATUS_CODE)
  references CIMS_REQUEST_STATUS (REQUEST_STATUS_CODE);
alter table CIMS_REQUEST
  add constraint FK_REQUEST_VERSION foreign key (VERSION_CODE)
  references CIMS_VERSION (VERSION_CODE);

