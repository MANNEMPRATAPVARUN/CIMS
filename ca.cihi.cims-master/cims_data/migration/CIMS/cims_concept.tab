create table CIMS_CONCEPT
(
  CONCEPT_ID          NUMBER not null,
  CONCEPT_CODE        VARCHAR2(20),
  CLASSIFICATION_CODE VARCHAR2(20)
)
;
alter table CIMS_CONCEPT
  add constraint PK_CONCEPT primary key (CONCEPT_ID);
alter table CIMS_CONCEPT
  add constraint FK_CONCEPT_CLASSIFICATION_CODE foreign key (CLASSIFICATION_CODE)
  references CIMS_CLASSIFICATION (CLASSIFICATION_CODE);
create index IE_CONCEPT_CODE on CIMS_CONCEPT (CLASSIFICATION_CODE, CONCEPT_CODE);

