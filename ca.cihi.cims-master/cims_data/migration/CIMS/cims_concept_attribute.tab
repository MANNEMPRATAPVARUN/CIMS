create table CIMS_CONCEPT_ATTRIBUTE
(
  VERSION_CODE        VARCHAR2(4) not null,
  CONCEPT_ID          NUMBER not null,
  CONCEPT_TYPE_CODE   VARCHAR2(10),
  CA_ENHANCEMENT_FLAG VARCHAR2(1),
  DAGGER_ASTERISK     VARCHAR2(1),
  PARENT_CONCEPT_ID   NUMBER,
  UNIT_CONCEPT_ID     NUMBER
)
;
alter table CIMS_CONCEPT_ATTRIBUTE
  add constraint PK_CONCEPT_ATTRIBUTE primary key (VERSION_CODE, CONCEPT_ID);
alter table CIMS_CONCEPT_ATTRIBUTE
  add constraint FK_CONCEPT_ATTRIBUTE_ID foreign key (CONCEPT_ID)
  references CIMS_CONCEPT (CONCEPT_ID) on delete cascade;
alter table CIMS_CONCEPT_ATTRIBUTE
  add constraint FK_CONCEPT_ATTRIBUTE_PARENT foreign key (VERSION_CODE, PARENT_CONCEPT_ID)
  references CIMS_CONCEPT_ATTRIBUTE (VERSION_CODE, CONCEPT_ID);
alter table CIMS_CONCEPT_ATTRIBUTE
  add constraint FK_CONCEPT_ATTRIBUTE_VERSION foreign key (VERSION_CODE)
  references CIMS_VERSION (VERSION_CODE);

